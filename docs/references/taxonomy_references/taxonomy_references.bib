@article{productionlaboratory_pollen_nodate,
	title = {{POLLEN} {MORPHOLOGY} {OF} {THE} {FAMILY} {BOMBACACEAE} {H}, {P}. {FUCHS}},
	abstract = {The justification for separating the family Bombacaceae from the Sterculiaceae, Malvaceae and Tiliaceae, which has long been doubted, now seems to be provided by palynomorphological characteristics. Examination of these characteristics suggests that a number of genera should be transferred from the Bombacaceae to the Sterculiaceae (Camptostemon, Matisia, pro parte) or to the Malvaceae (Hampea). Moreover, the palynomorphological characters exhibited by members of the family Bombacaceae point to triphyletic origin of the family, with south-Central America, East Africa + Madagascar and southeast Asia as centres. A number of phytogeographical conclusions can be drawn from the distribution of these group types in Tertiary and Recent time.},
	language = {en},
	author = {ProductionLaboratory, ShellExplorationand},
	file = {ProductionLaboratory - POLLEN MORPHOLOGY OF THE FAMILY BOMBACACEAE H, P. .pdf:files/8433/ProductionLaboratory - POLLEN MORPHOLOGY OF THE FAMILY BOMBACACEAE H, P. .pdf:application/pdf},
}

