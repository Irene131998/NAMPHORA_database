	pages = {341--366},
	file = {Gonçalves-Esteves et al. - 2021 - Pollen morphology and evolutionary history of Sapi.pdf:files/8438/Gonçalves-Esteves et al. - 2021 - Pollen morphology and evolutionary history of Sapi.pdf:application/pdf},
}

