@article{verhoeven_pollen_2001,
	title = {Pollen {Morphology} of the {Periplocoideae}, {Secamonoideae}, and {Asclepiadoideae} ({Apocynaceae})},
	volume = {88},
	issn = {00266493},
	url = {https://www.jstor.org/stable/3298634?origin=crossref},
	doi = {10.2307/3298634},
	abstract = {Among Apocynaceae the Periplocoideae are distinguished from the Secamonoideae and Asclepiadoideae by the presence of free tetrads or pollinia, which are shed onto spoon-shaped translators after anthesis. The Secamonoideae are distinguished from Asclepiadoideae by having four pollinia attached to a translator, rather than two pollinia as in Asclepiadoideae. In the Periplocoideae the exine consists of a tectum and a granular stratum. In Secamonoideae the exine consists of three layers: an outer tectum, subtended by a well-developed granular layer, which is followed by a foot layer. In the Asclepiadoideae the exine consists of a tectum (distal exine layer), a thin granular layer, and an inner foot layer.},
	language = {en},
	number = {4},
	urldate = {2025-03-25},
	journal = {Annals of the Missouri Botanical Garden},
	author = {Verhoeven, Rudolf L. and Venter, H. Johan T.},
