	pages = {659--673},
	file = {Yang et al. - 2020 - Pollen atlas for selected subfamilies of Euphorbia.pdf:files/8540/Yang et al. - 2020 - Pollen atlas for selected subfamilies of Euphorbia.pdf:application/pdf},
}

