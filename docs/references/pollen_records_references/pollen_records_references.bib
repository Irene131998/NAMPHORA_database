}

@article{parker_holocene_2004,
	title = {Holocene vegetation dynamics in the northeastern {Rub}' al‐{Khali} desert, {Arabian} {Peninsula}: a phytolith, pollen and carbon isotope study},
	volume = {19},
	issn = {0267-8179, 1099-1417},
	shorttitle = {Holocene vegetation dynamics in the northeastern {Rub}' al‐{Khali} desert, {Arabian} {Peninsula}},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/jqs.880},
	doi = {10.1002/jqs.880},
	abstract = {The Holocene vegetation history of the Arabian Peninsula is poorly understood, with few palaeobotanical studies to date. At Awaﬁ, Ras al-Khaimah, UAE, a 3.3 m lake sediment sequence records the vegetation development for the period 8500 cal. yr BP to \$ 3000 cal. yr BP.  13C isotope, pollen and phytolith analyses indicate that C3 Pooid grassland with a strong woody element existed during the early Holocene (between 8500 and 6000 cal. yr BP) and became replaced by mixed C3 and C4 grasses with a strong C4 Panicoid tall grass element between 5900 and 5400 cal. yr BP. An intense, arid event occurred at 4100 cal. yr BP when the lake desiccated and was inﬁlled by Aeolian sand. From 4100 cal. yr BP the vegetation was dominated by C4 Chloridoid types and Cyperaceae, suggesting an incomplete vegetation cover and Aeolian dune reactivation owing to increased regional aridity. These data outline the ecosystem dynamics and carbon cycling in response to palaeomonsoon and north-westerly variability during the Holocene. Copyright ß 2004 John Wiley \& Sons, Ltd.},
	language = {en},
	number = {7},
	urldate = {2025-03-25},
	journal = {J Quaternary Science},
	author = {Parker, A. G. and Eckersley, L. and Smith, M. M. and Goudie, A. S. and Stokes, S. and Ward, S. and White, K. and Hodson, M. J.},
	month = oct,
	year = {2004},
	pages = {665--676},
	file = {Parker et al. - 2004 - Holocene vegetation dynamics in the northeastern R.PDF:files/7997/Parker et al. - 2004 - Holocene vegetation dynamics in the northeastern R.PDF:application/pdf},
}

