	file = {Linstädter and Kehl - 2012 - The Holocene archaeological sequence and sedimento.PDF:files/8043/Linstädter and Kehl - 2012 - The Holocene archaeological sequence and sedimento.PDF:application/pdf},
}

@article{lippi_palaeoenvironmental_2011,
