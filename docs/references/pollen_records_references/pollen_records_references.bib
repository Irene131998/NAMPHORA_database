}

@article{burjachs_dinamica_nodate,
	title = {Dinámica de la vegetación durante el {Holoceno} en la isla de {Mallorca}},
	abstract = {Vegetation evolution through pollen analysis tluring the last 7000 years is showed in the first 8-meter core extracted from the "Albufera dlAlcúdia" (Majorca). This evolution is characterized by a strong substitution, at 6000 years BP, of mesophyllous vegetation by a more mediterranean conditions adapted vegetation. The taxa change is done with complexity. In a first pliase, Junlperus, Ephedra and Buxus are substituted by Olea and Pinus. Climatic reasons could be considered for this phenomenon. In a second step, mesophyllous taxa, like deciduous Quercus, Alnus, Betula and Ulmus,tend to diminish or even to extinguish (Fagus, Corylus).The anthropic action plays an important role during this phase.},
