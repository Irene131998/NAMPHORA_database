@article{benslama_nouvelles_2010,
	title = {Nouvelles contributions à l’histoire tardiglaciaire et holocène de la végétation en {Algérie} : analyses polliniques de deux profils sédimentaires du complexe humide d’{El}-{Kala}},
	volume = {333},
