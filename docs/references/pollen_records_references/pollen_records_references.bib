@article{marks_cyclonic_2019,
	title = {Cyclonic activity over northeastern {Africa} at 8.5–6.7 cal kyr {B}.{P}., based on lacustrine records in the {Faiyum} {Oasis}, {Egypt}},
	volume = {528},
